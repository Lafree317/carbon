local Carbon = (...)
local Vector2 = Carbon.Math.Vector2

local Bloop = Carbon.OOP:Class()
	:Members {
		Center = Vector2(0, 0),
		Radius = 1
	}

function Bloop:GetExtents()
	return Vector2(self.Center->xy - self.Radius), Vector2(self.Center->xy + self.Radius)
end

local a = Bloop:New()
print(a:GetExtents())